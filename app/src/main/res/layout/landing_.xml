<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="wrap_content">

	<TextView
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="EcoRecycle"
		android:textSize="50dp"
		android:layout_marginTop="100dp"
		android:layout_marginLeft="75dp"
		>

	</TextView>


	<RelativeLayout
		android:id="@+id/group_221"
		android:layout_width="500dp"
		android:layout_height="500dp"
		android:layout_marginStart="-275dp"
		android:layout_marginTop="250dp">


		<RelativeLayout
			android:id="@+id/group_2"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="1dp"
			android:layout_marginTop="191dp">

			<Button
				android:id="@+id/button4"
				android:layout_width="272.19dp"
				android:layout_height="50.03dp"
				android:layout_marginStart="0dp"
				android:layout_marginTop="0dp"
				android:visibility="invisible"
				android:text="Button" />

			<View
				android:id="@+id/rectangle_5"
				android:layout_width="272.19dp"
				android:layout_height="50.03dp"
				android:layout_marginStart="0dp"
				android:layout_marginTop="0dp"
				android:background="@drawable/rectangle_5_shape" />
			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="100dp"
				android:layout_marginTop="10dp"
				android:text="Entrar"
				android:textColor="@color/white"
				android:textSize="24dp">

			</TextView>

		</RelativeLayout>

		<View
			android:id="@+id/rectangle_7"
			android:layout_width="271.04dp"
			android:layout_height="47.83dp"
			android:layout_marginStart="0dp"
			android:layout_marginTop="0dp"
			android:background="@drawable/rectangle_7_shape" >
		</View>

		<View
			android:id="@+id/rectangle_8"
			android:layout_width="271.04dp"
			android:layout_height="47.83dp"
			android:layout_marginStart="0dp"
			android:layout_marginTop="94dp"
			android:background="@drawable/rectangle_8_shape" />


		<EditText
			android:id="@+id/editTextTextEmailAddress"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="10dp"
			android:ems="10"
			android:inputType="textEmailAddress" />
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="75dp"
			android:text="Email"
			android:textSize="24dp">

		</TextView>

		<EditText
			android:id="@+id/editTextTextPassword"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="10dp"
			android:layout_weight="1"
			android:ems="10"
			android:inputType="textPassword"
			android:layout_marginTop="100dp"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="75dp"
			android:layout_marginTop="100dp"
			android:text="Senha"
			android:textSize="24dp">

		</TextView>
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="Sign In And start your \n Airmed Experience"
			android:textSize="30dp"
			android:layout_marginTop="300dp"
			android:layout_marginLeft="0dp"
			>

		</TextView>

		<RelativeLayout
			android:id="@+id/group_218"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="233dp"
			android:layout_marginTop="111dp">

			<ImageView
				android:id="@+id/vector_ek1"
				android:layout_width="18.76dp"
				android:layout_height="16.58dp"
				android:layout_marginStart="0dp"
				android:layout_marginTop="0dp"
				android:contentDescription="vector_ek1"
				android:src="@drawable/vector_ek1" />

			<ImageView
				android:id="@+id/vector_ek2"
				android:layout_width="5.14dp"
				android:layout_height="5.14dp"
				android:layout_marginStart="8dp"
				android:layout_marginTop="7dp"
				android:contentDescription="vector_ek2"
				android:src="@drawable/vector_ek2" />

		</RelativeLayout>


		<RelativeLayout
			android:id="@+id/person_24px"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginStart="231dp"
			android:layout_marginTop="10dp">

			<ImageView
				android:id="@+id/icon_social_person_24px"
				android:layout_width="15.01dp"
				android:layout_height="15.52dp"
				android:layout_marginStart="3.75dp"
				android:layout_marginTop="3.88dp"
				android:contentDescription="icon_social_person_24px"
				android:src="@drawable/icon_social_person_24px" />

		</RelativeLayout>

	</RelativeLayout>


</LinearLayout>